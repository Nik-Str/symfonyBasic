<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1"/>
		<title>Symfony Basic</title>
		<link rel="icon" type="image/x-icon" href={{ asset('build/images/favicon.b3aa3301.ico') }}/>
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css">
		{% block stylesheets %}
			<link rel="stylesheet" href={{ asset('build/app.css') }}>
		{% endblock %}
		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{{ component('header') }}
		<main>
			{% block body %}{% endblock %}
		</main>
		{{ component('footer') }}
	</body>
</html>
